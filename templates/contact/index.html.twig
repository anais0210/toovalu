{% block body %}

    {{ form_start(form) }}

<section id="fh5co-contact" data-section="contact">
    <div class="container">
        <div class="row">
            <div class="col-md-12 section-heading text-center">
                <h2 class="to-animate">Contact</h2>
                <div class="row">
                    <div class="col-md-8 col-md-offset-2 subtext to-animate">
                        <h3>UNE DÉMONSTRATION ? NOUS RENCONTRER ?</h3>
                    </div>
                </div>
            </div>
        </div>
        <div class="row row-bottom-padded-md">
            <div class="col-md-6 to-animate">
                <h3>Contact</h3>
                <ul class="fh5co-contact-info">
                    <li class="fh5co-contact-address ">
                        <i class="icon-home"></i>
                        c/o Groupe KERAN 4 rue René Viviani 44200 Nantes
                    </li>
                    <li><i class="icon-phone"></i>02.51.17.81.80</li>
                    <li><i class="icon-envelope"></i>contact@toovalu.com</li>
                </ul>
            </div>

            <div class="col-md-6 to-animate">
                <h3>Contact</h3>
                <div class="form-group ">
                    <label for="name" class="sr-only"></label>
                    {{ form_widget(form.firstName, {'attr': {'placeholder': 'Nom'}}) }}
                    {{ form_errors(form.firstName) }}
                </div>
                <div class="form-group ">
                    <label for="name" class="sr-only"></label>
                    {{ form_widget(form.lastName, {'attr': {'placeholder': 'Prénom'}}) }}
                    {{ form_errors(form.lastName) }}
                </div>
                <div class="form-group ">
                    <label for=email" class="sr-only"></label>
                    {{ form_widget(form.email, {'attr': {'placeholder': 'Email'}}) }}
                    {{ form_errors(form.email) }}
                </div>
                <div class="form-group ">
                    <label for="message" class="sr-only">Message</label>
                    {{ form_widget(form.message, {'attr': {'placeholder': 'Message'}}) }}
                    {{ form_errors(form.message) }}
                </div>
                <div class="form-group ">
                    <input class="btn btn-primary btn-lg" value="Envoyer" type="submit">
                </div>
            </div>
        </div>

    </div>
    <div id="map" class="to-animate"></div>

    {{ form_end(form) }}
    {% endblock %}
